{
  "version": 8,
  "sources": {
    "baremaps": {
      "type": "vector",
      "url":"./tileset.json"
    }
  },
  "layers": [
    {
      "id": "trees",
      "type": "circle",
      "source": "trees",
      "source-layer": "trees",
      "minzoom": 13,
      "maxzoom": 20,
      "paint": {
        "circle-radius": 6,
        "circle-color": [
          "match",
          ["get", "type"],
          "Oak", "#33a02c",
          "Maple", "#1f78b4",
          "Pine", "#e31a1c",
          "Birch", "#ff7f00",
          "Spruce", "#6a3d9a",
          "Poplar", "#fdbf6f",  
          "Cypress", "#cab2d6",
          "Cedar", "#ffff99"
        ]
      }
    }
  ]
}

